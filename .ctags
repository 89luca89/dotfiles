--recurse=yes
--exclude=.git
--exclude=.gem
--exclude=node_modules
--exclude=vendor
--exclude=workspace
--exclude=*_compiled
--exclude=*.md
--exclude=*.xml
--exclude=*.json
--exclude=*.lock
--exclude=*.csv
--exclude=*.txt
--exclude=*.log
--exclude=*.jpg
--exclude=*.png
--exclude=*.jpeg
--exclude=*.gif
--exclude=*.bmp
--exclude=*.pdf
--exclude=*.doc
--exclude=*.docx
--exclude=*.sql
--exclude=libraries
--exclude=*.less
--exclude=*.scss
--exclude=*.css
--exclude=*/build/*
--exclude=*pyc
--fields=+l
--langmap=scheme:+.rkt
--langdef=ansible
--langmap=ansible:.yml.yaml
--regex-ansible=/^[ \t]*-[ \t]*name:[ \t]*(.+)/\1/k,tasks/
--langdef=Go
--langmap=Go:.go
--regex-Go=/func([ \t]+\([^)]+\))?[ \t]+([a-zA-Z0-9_]+)/\2/d,func/
--regex-Go=/var[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,var/
--regex-Go=/type[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,type/
--languages=ansible,python,Go,Make,C++,C
