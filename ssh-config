  HostName luca-lan.privatedns.org
  IdentityFile /home/luca-linux/.ssh/id_ecdsa
  Port 50364
  User fileserver

Host sftp
  HostName luca-lan.privatedns.org
  IdentityFile /home/luca-linux/.ssh/id_ecdsa
  Port 50364
  User fileserver

Host luca-lan
  HostName luca-lan.lan
  Port 22
  ProxyJump tunnel
  User root

Host luca-lan-roma
  HostName localhost
  IdentityFile /home/luca-linux/.ssh/id_ecdsa
  Port 3344
