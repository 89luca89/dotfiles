  User root

Host management
  HostName luca-lan.privatedns.org
  IdentityFile /home/luca-linux/.ssh/id_ecdsa
  Port 50365
  User bastion

Host tunnel
  HostName luca-lan.privatedns.org
  IdentityFile /home/luca-linux/.ssh/id_ecdsa
  Port 50363
  User bastion

Host emergency-tunnel
  HostName localhost
  IdentityFile /home/luca-linux/.ssh/id_ecdsa
  Port 2200
  User bastion

# global configuration
Host *
  ForwardAgent yes
  Compression yes
  ConnectTimeout 5
  ControlMaster yes
  ControlPath ~/tmp/.ssh/cm/%h-%p-%r.sock
  ControlPersist yes
  GSSAPIAuthentication no
  MACs hmac-sha2-256
