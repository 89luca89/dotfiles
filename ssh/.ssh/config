Include config.d/*

Host fileserver
  HostName fileserver
  ProxyJump jump
  # ProxyJump failover
  User fileserver

Host jump
  HostName luca-lan.homelinuxserver.org
  # HostName luca-lan.privatedns.org
  # HostName luca-lan.mooo.com
  Port 50363
  User bastion

Host failover
  HostName wifi-failover.homelinuxserver.org
  # HostName wifi-failover.mooo.com
  Port 50554
  User bastion

Host syncthing
  Hostname raspberrypi-1.lan
  User root
  Port 22
  LocalForward 8385 127.0.0.1:8384
  LocalForward 8386 192.168.100.71:8384
  ProxyJump jump

# global configuration
Host *
  Compression yes
  ConnectTimeout 15
  ForwardAgent yes
  IdentityFile ~/.ssh/id_ecdsa
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
