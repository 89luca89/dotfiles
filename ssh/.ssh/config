Host w540.lan
  HostName 192.168.100.135
  ProxyJump jump
  User root

Host yoga.lan
  HostName 192.168.1.101
  User root

Host luca-lan
  HostName luca-lan.lan
  Port 22
  ProxyJump jump
  User root

Host fileserver
  HostName fileserver
  ProxyJump jump
  User fileserver

Host jump
  HostName luca-lan.homelinuxserver.org
  # HostName luca-lan.privatedns.org
  # HostName luca-lan.mooo.com
  Port 50363
  User bastion

Host jump-roma
  HostName localhost
  Port 3343
  ProxyJump jump
  User root

Host failover
  HostName wifi-failover.homelinuxserver.org
  # HostName wifi-failover.mooo.com
  Port 50554
  User bastion

Host failover-roma
  HostName localhost
  Port 3343
  ProxyJump failover
  User root

# global configuration
Host *
  Compression yes
  ConnectTimeout 5
  ForwardAgent yes
  IdentityFile ~/.ssh/id_ecdsa
