Include config.d/*

Host fileserver
  HostName fileserver
  ProxyJump jump
  # ProxyJump failover
  User fileserver

Host jump
  HostName wifi-failover.homelinuxserver.org
  # HostName wifi-failover.mooo.com
  Port 30686
  User bastion

Host raspberrypi-5
  User luca-linux
  ProxyJump jump
  Hostname 192.168.1.124
  Port 22

Host raspberrypi-2
  User root
  ProxyJump jump
  Hostname 192.168.1.237
  Port 22

Host raspberrypi-1
  User root
  ProxyJump raspberrypi-2
  Hostname 100.75.96.62
  Port 22

Host syncthing
  Hostname 192.168.1.237
  User root
  Port 22
  LocalForward 8080 127.0.0.1:80
  LocalForward 8385 127.0.0.1:8384
  LocalForward 8386 192.168.1.75:8384

Host thinkpad
  Hostname 192.168.1.46
  LocalForward 8383 127.0.0.1:8384
  ProxyJump jump
  Port 22

Host xps
  Hostname 192.168.1.102
  ProxyJump jump
  Port 22

Host xps2
  Hostname localhost
  Port 2222

Host libvirt
  Hostname 10.88.0.2
  Port 22
  User root

# global configuration
Host *
  Compression yes
  ConnectTimeout 15
  ForwardAgent yes
  IdentityFile ~/.ssh/id_ecdsa
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
  # ControlMaster auto
  # ControlPath /tmp/ssh-%r@%h:%p
