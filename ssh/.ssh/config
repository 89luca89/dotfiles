Include config.d/*

Host syncthing
  Hostname raspberrypi-2
  User root
  Port 22
  ForwardAgent yes
  LocalForward 8080 127.0.0.1:80
  LocalForward 8383 100.75.96.62:8384
  LocalForward 8385 127.0.0.1:8384
  LocalForward 8386 192.168.1.75:8384

Host .host
    ProxyCommand /usr/lib/systemd/systemd-ssh-proxy unix/run/ssh-unix-local/socket %p
    ProxyUseFdpass yes
    CheckHostIP no

Host unix/* unix%* vsock/* vsock%* vsock-mux/* vsock-mux%*
	ProxyCommand /usr/lib/systemd/systemd-ssh-proxy %h %p
	ProxyUseFdpass yes


# global configuration
Host *
  Compression yes
  ConnectTimeout 15
  IdentityFile ~/.ssh/id_ecdsa
  IdentityFile ~/.ssh/id_rsa
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
  ControlMaster auto
  ControlPath /tmp/ssh-%r@%h:%p
