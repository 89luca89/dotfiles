Include config.d/*

Host fileserver
  HostName fileserver
  ProxyJump jump
  # ProxyJump failover
  User fileserver

Host jump
  HostName wifi-failover.homelinuxserver.org
  # HostName wifi-failover.mooo.com
  Port 22222
  User bastion

Host syncthing
  Hostname raspberrypi-2.lan
  User root
  Port 22
  LocalForward 8385 127.0.0.1:8384
  LocalForward 8386 thinkpad-local:8384
  ProxyJump jump

# global configuration
Host *
  Compression yes
  ConnectTimeout 15
  ForwardAgent yes
  IdentityFile ~/.ssh/id_ecdsa
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
